{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.lib",
  "include": ["src/*.ts"],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "outDir": "dist",
    "sourceMap": true,

    "noCheck": true,

    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.build.tsbuildinfo"
  }
}
